<div class="new-lists">
  <div class="new-list">
    <h2>Edit Movie</h2>
    <%= simple_form_for [ @movie ] do |f| %>
      <%= f.input :title %>
      <%= f.input :overview %>
      <%= f.input :rating %>
      <%= f.input :poster_url %>
      <%= f.submit "Submit", class: "btn btn-light" %>
    <% end %>
  </div>
</div>
